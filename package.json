{
  "private": true,
  "name": "barbell-loader",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "build:gh": "GITHUB_PAGES=barbell-loader preact build && dot-json ./build/manifest.json start_url \"/\""
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "lodash": "^4.17.15",
    "preact": "^10.5.5",
    "preact-cli": "^3.0.3",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "^5.1.10"
  },
  "devDependencies": {
    "dot-json": "^1.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
